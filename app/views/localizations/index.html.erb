<h1>Listing localizations</h1>

<table>
  <tr>
    <th>Address</th>
    <th>Gmaps</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @localizations.each do |localization| %>
  <tr>
    <td><%= localization.address %></td>
    <td><%= localization.gmaps %></td>
    <td><%= localization.latitude %></td>
    <td><%= localization.longitude %></td>
    <td><%= link_to 'Show', localization %></td>
    <td><%= link_to 'Edit', edit_localization_path(localization) %></td>
    <td><%= link_to 'Destroy', localization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Localization', new_localization_path %>
